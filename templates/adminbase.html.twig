<!DOCTYPE html>
<html lang="en" >
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">

		<title>
			{% block title %}Association 100% Famille |
			{% endblock %}
		</title>
		<link href="https://fonts.googleapis.com/css2?family=Urbanist:wght@300;400;500;600;700;900&display=swap" rel="stylesheet">
		<link rel="shortcut icon" href="{{asset("images/favicon/favicon.ico")}}" type="image/x-icon">



        {% block javascripts %}
            {% block importmap %}{{ importmap('admin') }}{% endblock %}
        {% endblock %}
	</head>
	<body  class="admin" {{stimulus_controller('theme_switch')}}>


		{% include "partials/admin/components/_aside.html.twig" with {theme:theme} %}

		<div class="container container--{{theme}}">
		{% include "partials/admin/components/_header.html.twig" with {
			title: block('pagetitle') ?? 'Titre de la page',
			theme: theme
		} %}
			<main>
					{% include "partials/admin/components/_flash.html.twig" with {
						flashes: app.flashes,
						
					} %}
				
					{% block main %}{% endblock %}
			</main>
		</div>
	</body>
</html>
